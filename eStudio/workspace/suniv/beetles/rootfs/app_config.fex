;软件版本号，方案商使用，客户不需要更改
[prog_ver]
ver=string:.G01.1500

;对于已经有做进软件的按键改变key_type的值即可，
;软件坐进去的按键有数值1表示KEY_01, 2-->KEY_02, 3-->KEY_03，
;8-->KEY_08, 9-->KEY_09, 21-->KEY_21,   25-->KEY_25, 46-->KEY_46,
;22-->KEY_22EJ,23-->KEY_23EJ ,24-->KEY_24EJ,928-->LZD_KEY_928
;48-->KEY_48,36-->KEY36,3601-->KEY3601,39-->KEY39,49-->KEY49,51-->KEY51,52-->KEY52,63-->KEY63
;53-->KEY53,56-->D592KEY56,26-->KEY26,59-->KEY59,60-->KEY60,65-->KEY65,67-->KEY67,
;66-->KEY66,68-->KEY68,69-->KEY69,70-->KEY70,71-->KEY71,76-->KEY76,94-->KEY94,93-->KEY93,9301-->KEY9301
;10103-->KEY_101_03正线， 110103-->KEY_101_03反线
;(目前软件只有以上几种类型)
[key_para]
key_type = 67
 
;ad按键x1路，根据阻值从小到大 按键从上往下排列,目前每路可做
;12个按键，客户修改时，重新写“string:”号后面的字符。
;电阻从0开始往下，若碰到第一个电阻不是0的 将字符改成"___ "
如“KEY1=string:___”更改下面的按键请将 上面的key_type设为0
[key_para_x1]
KEY1=string:MUSIC
KEY2=string:MOVIE
KEY3=string:FM
KEY4=string:RECORD
KEY5=string:PHOTO
KEY6=string:VOICEUP
KEY7=string:VOICEDOWN
KEY8=string:LEFT
KEY9=string:RIGNT
KEY10=string:ENTER
KEY11=string:TV
KEY12=string:DVD

;ad按键x2路，根据阻值从小到大 按键从上往下排列
[key_para_x2]
KEY1=string:NUM1
KEY2=string:NUM2
KEY3=string:NUM3
KEY4=string:NUM4
KEY5=string:NUM5
KEY6=string:NUM6
KEY7=string:NUM7
KEY8=string:NUM8
KEY9=string:NUM9
KEY10=string:NUM0
KEY11=string:MENU
KEY12=string:SUBMENU

;遥控按键 用户码请填写反码,code是码值，ir_key是按键
;code和ir_key一一对应
;客户修改时重新写code后的十六进制数和"string:"后的字符，
;对于多出的按键写成如“code44=0x00   ir_key44=string:___”
;对于已经做好的遥控，改变遥控类型ir_type的值和对应用户码，
;目前软件包含的类型有 1--IR_01,108--IR_0108,     
;101--IR_TEST_01,  19--IR_19, 191--IR_19EJ, 23--IR_23,16--IR_16
;24--IR_24 ,  43--IR_43 , 50-->IR_50,46-->IR_46,47--IR47,29-->IR_29
;42--IR_42,4201--IR_4201,4202--IR_4202,14--IR_14,6--IR_06,45--IR_45,
;13--IR_13
;35-->IR_35,34-->IR_34,36-->IR_36,39-->IR_39,20-->IR_20
;0xf708   2--IR_02, 4--IR_04,401--IR04EJ
;0xbf40   41-->IR_41,25-->IR_25
