[system]
;fm search thread hold, (1-63) default is 4，灵敏度客户不需要更改
fm_th=4
;FM6207灵敏度 范围0x0000~0xffff 推荐参数 0x3848 0x4058 0x4878
sensibility_6207=0x4878
;5807的灵敏度参数值可设为0xc0~0xc6
sensibility_5807=0xc5

;enable restore default param when poweron.
;参数改变后重新上电开机参数不需保存将相应位置1
;bit[0,7]:   {mediaType, videoScreenRatio, volume, lang, style, keytone, cvbs, light_off}
;bit[8,14]: {pwm, bright, contrast, hue, saturability, tv_system, avin_system}
enRestoreParam=0x0007

;default auto play media type,range:[0,3]:{video,audio,ebook,photo}
;自动播放媒体类型0是视屏1是音乐2是电子书3是图片
mediaType=0

;default video screen ratio.
;0,FULL_SCREEN_VIDEO_RATIO，全屏，按视频比例显示
;1,FULL_SCREEN_SCREEN_RATIO，全屏，按屏幕比例显示
;2,ORIGIN_SIZE，按原始尺寸显示
;3,4:3,  4:3比例显示 可能会导致变形
;4,16:9 ，16:9比例显示，可能会导致变形
videoScreenRatio=1

;disable display logo, range:{0,1}:{display, blank}
;开机logo显示，1不显示，0显示
logoDisable=0

;real maximum volume, range:[0, 30]
;音量输出等级
realVolume_max=28

;default volume 0-30
;音量默认级数
volume=30

;default language 0x400/0x410/0x420 en/chs/cht
;默认语言0x400英文0x410简体中文0x420繁体中文
lang=0x410

;default background 0-1
;默认背景，对于只有一个背景的软件，该值为0就可以，不需更改
style=0

;default keytone off/on 0/1
;按键音,客户不需更改
keytone=0

;default cvbs output ntsc/pal 0/1
;制式 0为N制 1为P制
cvbs=1

;backlight_off time, range:{0,10,30,60,90}
;背光自动关闭时间可以设 0不关，10表示10,30表示30秒，
;60表示60秒，90表示90秒
light_off=60

;default lcd pwm, range:[0,14]
;VCOM默认值
pwm=10

;default display bright, range:[1,100]
;亮度默认值
bright=60

;default display contrast, range:[1,100]
;对比度默认值
contrast=50

;default display hue, range:[1,100]
;色度默认值
hue=40

;default display saturability, range:[1,100]
;饱和度默认值
saturability=50

;default tv system, range:[0,8]:{PAL-DK/PAL-I/PAL-BG/NTSC-M/SECAM-DK/PAL-M/PAL-N/PAL-60/SECAM-BG}
;电视制式，依次对应0,1,2,3,4,5,6,7,8
tv_system=0

;default avin system, range:[0,8]:{PAL-DK/PAL-I/PAL-BG/NTSC-M/SECAM-DK/PAL-M/PAL-N/PAL-60/SECAM-BG}
;AVIN 制式
avin_system=1

;fm 自动关屏时间，该项无用，客户不需更改
[fm]
;;auto close screen in fm, in 5s
;auto_close_scn=30
